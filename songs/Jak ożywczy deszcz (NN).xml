      <title>Jak ożywczy deszcz</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>NN</author>
    </authors>
    <songbooks>
      <songbook name="Duch Święty" entry="62"/>
    </songbooks>
    <themes>
      <theme>Duch Święty</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Jak ożywczy deszcz<br/>Duchu Święty przyjdź<br/>Dotknij naszych serc<br/>Rozpal ognia żar</lines>
    </verse>
    <verse name="v1b">
      <lines>Dziś przenikaj nas<br/>Tchnieniem mocy Swej<br/>Bądź nam światłem dnia<br/>I nadzieją serc</lines>
    </verse>
  </lyrics>
</song>
