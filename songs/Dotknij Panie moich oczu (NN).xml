  <properties>
    <titles>
      <title>Dotknij Panie moich oczu</title>
    </titles>
    <authors>
      <author>NN</author>
    </authors>
    <songbooks>
      <songbook name="Duch Święty" entry="39"/>
    </songbooks>
    <themes>
      <theme>Duch Święty</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Dotknij Panie moich <br/>oczu abym przejrzał	<br/>Dotknij Panie moich warg<br/>abym przemówił – uwielbieniem</lines>
    </verse>
    <verse name="v1b">
      <lines>Dotknij Panie mego serca <br/>i oczyść je<br/>Niech Twój Święty Duch <br/>dziś ogarnia mnie.</lines>
    </verse>
  </lyrics>
</song>
