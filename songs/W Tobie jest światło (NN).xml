  <properties>
    <titles>
      <title>W Tobie jest światło</title>
    </titles>
    <authors>
      <author>NN</author>
    </authors>
    <songbooks>
      <songbook name="inne" entry="185"/>
    </songbooks>
    <themes>
      <theme>inne</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>W Tobie jest światło,<br/>Każdy mrok rozjaśni. <br/>W Tobie jest życie, <br/>Ono śmierć zwycięża.</lines>
    </verse>
    <verse name="v2">
      <lines>Ufam Tobie, miłosierny,<br/>Jezu wybaw nas! </lines>
    </verse>
  </lyrics>
</song>
