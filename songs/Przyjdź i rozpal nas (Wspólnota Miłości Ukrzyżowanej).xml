<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.2.1" modifiedIn="OpenLP 2.2.1" modifiedDate="2018-03-09T17:28:57">
  <properties>
    <titles>
      <title>Przyjdź i rozpal nas</title>
      <title>Duchu Święty, o przyjdź</title>
    </titles>
    <authors>
      <author>Wspólnota Miłości Ukrzyżowanej</author>
    </authors>
    <songbooks>
      <songbook name="Duch Święty" entry="220"/>
    </songbooks>
    <themes>
      <theme>Duch Święty</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Duchu Święty</lines>
    </verse>
    <verse name="v2">
      <lines>O przyjdź i rozpal nas<br/>I ulecz nas<br/>Miłość nam daj !</lines>
    </verse>
    <verse name="v3">
      <lines>O przyjdź, napełnij nas<br/>Rozraduj nas<br/>Miłość nam daj !</lines>
    </verse>
    <verse name="v4">
      <lines>O przyjdź i utul nas<br/>Rozkochaj nas<br/>Miłość nam daj</lines>
    </verse>
  </lyrics>
</song>
