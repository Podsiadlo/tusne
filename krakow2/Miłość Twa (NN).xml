  <properties>
    <titles>
      <title>Miłość Twa</title>
    </titles>
    <authors>
      <author>NN</author>
    </authors>
    <songbooks>
      <songbook name="Uwielbienie"/>
    </songbooks>
    <themes>
      <theme>Uwielbienie</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Miłość Twa <br/>od najwyższych gór wyższa jest<br/>Wielka jest wierność Twa,<br/>do nieba sięga wzwyż</lines>
    </verse>
    <verse name="v1b">
      <lines>Miłość Twa, <br/>głębsza niż ocean bez dna,<br/>Wielka jest wiernośc Twa,<br/>gdy do mnie zniżasz się.</lines>
    </verse>
  </lyrics>
</song>
