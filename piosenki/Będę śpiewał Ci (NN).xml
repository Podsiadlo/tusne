<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.5" modifiedIn="OpenLP 2.4.5" modifiedDate="2016-10-07T13:41:45">
  <properties>
    <titles>
      <title>Będę śpiewał Ci</title>
      <title>Cała ziemie jest pełna</title>
    </titles>
    <authors>
      <author>NN</author>
    </authors>
    <songbooks>
      <songbook name="Uwielbienie" entry="211"/>
    </songbooks>
    <themes>
      <theme>Uwielbienie</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Będę śpiewał Ci<br/>Najpiękniejszą pieśń<br/>Z aniołami tak<br/>Pragnę śpiewać Ci</lines>
    </verse>
    <verse name="v1b">
      <lines>Święty, Święty, Święty jest Pan<br/>Zastępów Pan</lines>
    </verse>
    <verse name="v1c">
      <lines>Cała ziemia jest pełna Jego chwały</lines>
    </verse>
  </lyrics>
</song>
