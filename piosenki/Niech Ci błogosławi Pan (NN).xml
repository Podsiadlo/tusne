<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.5" modifiedIn="OpenLP 2.4.5" modifiedDate="2016-10-07T13:42:00">
  <properties>
    <titles>
      <title>Niech Ci błogosławi Pan</title>
    </titles>
    <authors>
      <author>NN</author>
    </authors>
    <songbooks>
      <songbook name="Czwarty czwartek"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Niech ci błogosławi Pan  		<br/>niechaj cię strzeże 			<br/>niech rozjaśni oblicze swe nad tobą 	<br/>i niech miłościw ci będzie	</lines>
    </verse>
    <verse name="v2">
      <lines>Niech ci błogosławi Pan<br/>niechaj cię strzeże<br/>niech obróci twarz swoją ku tobie<br/>i niech da ci swój pokój</lines>
    </verse>
    <verse name="c1">
      <lines>Niech ci błogosławi Pan			</lines>
    </verse>
  </lyrics>
</song>
